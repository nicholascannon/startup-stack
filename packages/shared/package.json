{
  "name": "@startup-stack/shared",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./api/*": {
      "types": "./dist/api/*.d.ts",
      "import": "./dist/api/*.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "typescript": "^5.6.3"
  }
}
